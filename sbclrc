; -*- mode: Lisp;-*-

(setf *read-default-float-format* 'double-float)

(sb-ext:restrict-compiler-policy 'debug 3)

(require 'asdf)
#-quicklisp
(let ((quicklisp-init
       (merge-pathnames "quicklisp/setup.lisp"
                        (user-homedir-pathname))))
  (when (probe-file quicklisp-init)
    (load quicklisp-init)))
